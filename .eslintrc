{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "max-len": [2, 120],
    "arrow-parens": ["off"],
    "no-use-before-define": "off",
    "promise/param-names": 2,
    "promise/always-return": 2,
    "promise/catch-or-return": 2,
    "promise/no-native": 0,
  },
  "plugins": [
    "import",
    "promise",
  ]
}
